{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react",
    "jsxFactory": "preact.createElement",
    "jsxFragmentFactory": "preact.Fragment",
    "isolatedModules": true,
    "strict": true,
    "noUncheckedIndexedAccess": true
  }
}
